{
     "version": 2,
     "builds": [
          {
               "src": "package.json",
               "use": "@vercel/node"
          }
     ],
     "routes": [
          {
               "src": "/(.*)",
               "dest": "/"
          }
     ],
     "rewrites": [
          {
               "source": "/(.*)",
               "destination": "/index.html"
          }
     ],
     "github": {
          "enabled": true,
          "silent": false
     },
     "framework": "vite"
}